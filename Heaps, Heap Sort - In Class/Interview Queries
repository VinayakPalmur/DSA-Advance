import java.io.*; // for handling input/output
import java.util.*; // contains Collections framework

// don't change the name of this class
// you can add inner classes if needed
class Main {
	public static void main (String[] args) {
                      // Your code here
		PriorityQueue<Integer> maxHeap = new PriorityQueue<>(Collections.reverseOrder());
		PriorityQueue<Integer> minHeap = new PriorityQueue<>();
		Scanner inputTaker = new Scanner(System.in);
		int n = inputTaker.nextInt();
		for(int i =0;i< n;i++){
			int queryType = inputTaker.nextInt();
			performQuery(queryType, maxHeap, minHeap, inputTaker);
		}

	}

	private static void performQuery(int queryType, PriorityQueue<Integer> maxHeap, PriorityQueue<Integer> minHeap, Scanner inputTaker){
		switch(queryType){
			case 1:{
				int x = inputTaker.nextInt();
				maxHeap.add(x);
				minHeap.add(x);
				break;
			}
			case 2:{
				int x = inputTaker.nextInt();
				if(maxHeap.contains(x)){
					maxHeap.remove(x);
					minHeap.remove(x);
				}else{
					System.out.println("-1");
				}
				break;
			}
			case 3:{
				if(maxHeap.isEmpty()){
					System.out.println("-1");
				}else{
					System.out.println(maxHeap.peek());
				}
				break;
			}
			case 4:{
				if(minHeap.isEmpty()){
					System.out.println("-1");
				}else{
					System.out.println(minHeap.peek());
				}
				break;
			}
		}
	}
}
